{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#121214"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@react-native-firebase/app"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-info.plist",
      "bundleIdentifier": "com.diamondApp",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "android": {
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121214"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.diamondApp"
    },
    "extra": {
      "eas": {
        "projectId": "96f87112-d7b7-4653-ba92-a2212ed665c4"
      }
    }
  }
}
